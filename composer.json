{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 9 projects with Composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": {
        "blt-pa11y": {
            "type": "vcs",
            "url": "https://github.com/nikunjkotecha/blt-pa11y.git",
            "no-api": true
        },
        "0": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "jquery-validation": {
            "type": "package",
            "package": {
                "name": "jquery-validation/jquery-validation",
                "version": "1.19.5",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/jquery-validation/jquery-validation/releases/download/1.19.5/jquery-validation-1.19.5.zip",
                    "type": "zip"
                }
            }
        }
    },
    "require": {
        "php": ">=7.4",
        "acquia/blt": "^13.5",
        "acquia/blt-behat": "^1.2",
        "acquia/blt-drupal-test": "^1.0",
        "alexku/blt-github-actions": "dev-master",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "drupal/clientside_validation": "^4.0",
        "drupal/core-composer-scaffold": "^9.3",
        "drupal/core-recommended": "^9.3",
        "drush/drush": "^11",
        "jquery-validation/jquery-validation": "^1.19",
        "se/selenium-server-standalone": "^3.141",
        "vlucas/phpdotenv": "^5.1",
        "webflo/drupal-finder": "^1.2"
    },
    "require-dev": {
        "behat/behat": "^3.13",
        "drupal/core-dev": "^9.3",
        "zaporylie/composer-drupal-optimizations": "^1.2"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "discard-changes": true,
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "acquia/blt": true,
            "drupal/core-composer-scaffold": true,
            "zaporylie/composer-drupal-optimizations": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": [
            "load.environment.php"
        ]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "docroot/"
            }
        },
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "acquia/blt-behat": {
                "Allow setting different example file per environment or other conditions.": "https://patch-diff.githubusercontent.com/raw/acquia/blt-behat/pull/31.patch"
            }
        }
    }
}
